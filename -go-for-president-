let age=90

  if (age < 18) {
     console.log(" готовься к взрослой жизни, хохохо");
   } 
   else if (age < 21)
  {
     console.log("ты можешь голосовать, гулять после 11 ночи, сдать права, пойти работать");
   }
 else if (age < 25) {
     console.log("ты можешь голосовать, работать, платить налоги, водить автомобиль, покупать алкаголь, сигареты,энергетики");
   } 
   else if (age < 30)
       {
     console.log(" бери на себя полную ответственность за свои действия!!!!!!!! создавай семью, путешествуй и наслаждайся жизнью.");
   }
    else if (age < 60) 
      {
     console.log("пенсия?????");
   } 
   else
    {
     console.log("можешь наслаждаться пенсией");
   }
